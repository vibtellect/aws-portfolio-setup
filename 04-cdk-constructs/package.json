    "test:tdd": "jest --watch --verbose --coverage=false",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "clean": "rm -rf lib node_modules",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write '**/*.{ts,json,md}'",
    "validate": "npm run build && npm run test"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "infrastructure-as-code",
