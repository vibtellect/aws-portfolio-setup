  "description": "Production-ready AWS CDK Constructs Library with security best practices and cost optimization",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest --coverage",
