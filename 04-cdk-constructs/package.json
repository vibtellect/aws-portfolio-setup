{
  "name": "@vibtellect/aws-cdk-constructs",
  "version": "2.0.0",
  "description": "Production-ready AWS CDK Constructs Library - 13/13 primitives complete with security best practices and cost optimization",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:tdd": "jest --watch --verbose --coverage=false",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:integration": "jest --config jest.integration.config.js --runInBand",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "localstack:start": "docker-compose up -d",
    "localstack:stop": "docker-compose down",
    "localstack:restart": "docker-compose restart",
    "localstack:logs": "docker-compose logs -f localstack",
    "localstack:health": "curl -s http://localhost:4566/_localstack/health | jq",
    "localstack:test": "npm run localstack:start && sleep 10 && npm run test:integration",
    "clean": "rm -rf lib node_modules",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write '**/*.{ts,json,md}'",
    "validate": "npm run build && npm run test"
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "infrastructure-as-code",
    "typescript",
    "aws-cdk",
    "cloud",
    "iac",
    "security",
    "cost-optimization"
  ],
  "author": "Vitalij",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vibtellect/aws-portfolio-setup",
    "directory": "04-cdk-constructs"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.120.0",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "^3.535.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.535.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.535.0",
    "@aws-sdk/client-dynamodb": "^3.535.0",
    "@aws-sdk/client-iam": "^3.535.0",
    "@aws-sdk/client-kms": "^3.535.0",
    "@aws-sdk/client-lambda": "^3.535.0",
    "@aws-sdk/client-route-53": "^3.535.0",
    "@aws-sdk/client-s3": "^3.535.0",
    "@aws-sdk/client-sns": "^3.535.0",
    "@aws-sdk/client-sqs": "^3.535.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "aws-cdk-lib": "^2.120.0",
    "aws-sdk-client-mock": "^3.0.1",
    "constructs": "^10.3.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
