    "security",
    "cost-optimization"
  ],
  "author": "Vitalij",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vibtellect/aws-portfolio-setup",
    "directory": "04-cdk-constructs"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.120.0",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "aws-cdk-lib": "^2.120.0",
    "constructs": "^10.3.0",
